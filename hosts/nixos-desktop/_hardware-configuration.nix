    device = "/dev/disk/by-uuid/22a2cf8b-2ca4-45b5-b6c4-cd88c1088716";
    fsType = "ext4";
  };

  fileSystems."/boot" = {
    device = "/dev/disk/by-uuid/5773-A83F";
    fsType = "vfat";
    options = ["fmask=0077" "dmask=0077"];
  };

  swapDevices = [
    {device = "/dev/disk/by-uuid/7498c596-4e2f-4439-9388-5736411faef9";}
